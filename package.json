{
  "name": "wouvy-api",
  "version": "0.5.0",
  "author": "Wouvy - Kevin Depuydt",
  "description": "Wouvy API",
  "main": "server.js",
  "keywords": [
    "Wouvy",
    "Node.js",
    "API",
    "JWT",
    "Express",
    "Babel"
  ],
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config -r @babel/register\"",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir -p dist && ./node_modules/.bin/babel src -s -D -d dist",
    "eslint": "eslint src",
    "test": "NODE_ENV=test mocha --timeout 10000 --compilers js:@babel/register test/api.test.js",
    "postinstall": "npm run -s build",
    "fixtures:load": "babel-node fixtures/load.js"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.17.2",
    "canvas": "^1.6.8",
    "chalk": "^2.1.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.4",
    "express-session": "^1.15.6",
    "helmet": "^3.8.1",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.4",
    "method-override": "^2.3.9",
    "mongoose": "^4.11.9",
    "mongoose-deep-populate": "^3.0.0",
    "mongoose-unique-validator": "^1.0.6",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "nodemailer": "^4.4.1",
    "nodemailer-express-handlebars": "^2.0.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth2": "^0.1.6",
    "passport-linkedin": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "socket.io": "^2.0.4",
    "validator": "^9.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^20.0.3",
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "mocha": "^4.0.0",
    "nodemon": "^1.11.0"
  }
}
